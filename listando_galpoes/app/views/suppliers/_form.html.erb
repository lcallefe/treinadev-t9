<%= form_with(model: @supplier) do |f| %>
    <div class="form-group">
      <%= f.label :corporate_name %>
      <%= f.text_field :corporate_name %>
    </div><br>

    <div class="form-group">
      <%= f.label :brand_name %>
      <%= f.text_field :brand_name %>
    </div><br>

    <div class="form-group">
      <%= f.label :registration_number %>
      <%= f.text_field :registration_number %>
    </div><br>

    <div class="form-group">
      <%= f.label :email %>
      <%= f.text_field :email %>
    </div><br>

    <div class="form-group">
      <%= f.label :city %>
      <%= f.text_field :city %>
    </div><br>

    <div class="form-group">
      <%= f.label :state %>
      <%= f.text_field :state %>
    </div><br>

    <div class="form-group">
      <%= f.label :full_address %>
      <%= f.text_field :full_address %>
    </div><br>
    
    <div class="form-group">
      <%= f.submit 'Enviar', class: "btn btn-default btn-primary" %>
    </div>
  <% end %>






